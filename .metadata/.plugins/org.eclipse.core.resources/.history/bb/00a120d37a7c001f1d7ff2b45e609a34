package Notdefault;

public class Starfromright {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i = 0; i < 10; i++) {
			for(int j = i ; j <= 10-i; j++) {
				System.out.printf("%d",j);
			}
//			for(int k = 0; k<i; k--) {
//				System.out.printf("*");
//			}
			System.out.println();
		}
	}

}
