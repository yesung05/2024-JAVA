package Notdefault;

public class Starfromright {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i = 10; i<0; i--) {
			for(int j = 0 ; j<i; j++) {
				System.out.printf(" ");
			}
			for(int k = 0; k<10-i; k++) {
				System.out.printf("*");
			}
			System.out.println();
		}
	}

}
